    "epochs = 200000\n",
    "for i in tqdm(range(epochs)):\n",
    "    # Create a random minibatch from our dataset\n",
    "    ix = torch.randint(0,Xtr.shape[0],(32,)) # 32 random indexes \n",
    "    # Forward Pass\n",
