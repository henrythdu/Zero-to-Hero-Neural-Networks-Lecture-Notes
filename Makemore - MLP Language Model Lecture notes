   "source": [
    "for _ in range(1000):\n",
    "    # Forward Pass\n",
    "    emb = C[X]\n",
    "    h = torch.tanh(emb.view(emb.shape[0],-1)@W1 + b1)\n",
    "    logits = h@W2 + b2\n",
    "    loss = F.cross_entropy(logits, Y)\n",
    "    # Backward pass\n",
    "    for p in parameters:\n",
    "        p.grad = None\n",
    "    loss.backward()\n",
    "\n",
    "    # Update the weight\n",
    "    for p in parameters:\n",
    "        p.data += -0.1*p.grad\n",
    "print(loss.item())"
   ]
  },
  {
