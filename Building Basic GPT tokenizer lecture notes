     "execution_count": 12,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "def encode(text):\n",
    "    tokens = list(text.encode(\"utf-8\"))\n",
    "    while len(tokens) >= 2: #Prevent a single character text\n",
    "        stats = get_stats(tokens) #Get the stats on the pair\n",
    "        pair = min(stats, key = lambda p: merges.get(p,float(\"inf\"))) #Find the pair with the lowest index in merges\n",
    "        if pair not in merges:\n",
    "            break\n",
    "        idx = merges[pair]\n",
    "        tokens = merge(tokens, pair, idx)\n",
    "    return tokens\n",
    "encode(\"Hello World !\")"
   ]
  },
