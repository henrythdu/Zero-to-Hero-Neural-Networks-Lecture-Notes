   "source": [
    "vocab = {idx: bytes([idx]) for idx in range(256)} #Vocab in bytes\n",
    "\n",
    "for (p0, p1), idx in merges.items():\n",
    "    vocab[idx] = vocab[p0] + vocab[p1] # Adding merged items\n",
    "\n",
    "def decode(ids):\n",
    "    # given ids (list of integers), return Python string\n",
    "    tokens = b\"\".join(vocab[idx] for idx in ids)\n",
    "    text = tokens.decode(\"utf-8\")\n",
    "    return text\n",
    "\n",
    "try:\n",
    "    print(decode([128])) \n",
    "except Exception as e: print(e)\n",
    "#This exception is due to byte representation of 128 is 1 follows by all 0, which is invalid for utf-8\n",
    "\n",
    "    \n",
    "def decode(ids):\n",
    "    # given ids (list of integers), return Python string\n",
    "    tokens = b\"\".join(vocab[idx] for idx in ids)\n",
    "    text = tokens.decode(\"utf-8\", errors = \"replace\")\n",
    "    return text\n",
    "\n",
    "print(decode([128]))"
   ]
