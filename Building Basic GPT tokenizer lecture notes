    "# Find the consecutive elements\n",
    "def get_stats(ids):\n",
    "    counts = {}\n",
    "    for pair in zip(ids,ids[1:]):\n",
    "        counts[pair] = counts.get(pair,0) + 1\n",
    "    return counts\n",
    "stats = get_stats(tokens)\n"
