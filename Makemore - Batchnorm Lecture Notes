  {
   "cell_type": "code",
   "execution_count": 35,
   "id": "50608553",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2024-07-08T15:02:39.561461Z",
     "iopub.status.busy": "2024-07-08T15:02:39.561068Z",
     "iopub.status.idle": "2024-07-08T15:02:39.708912Z",
     "shell.execute_reply": "2024-07-08T15:02:39.707834Z"
    },
    "papermill": {
     "duration": 0.172518,
     "end_time": "2024-07-08T15:02:39.711070",
     "exception": false,
     "start_time": "2024-07-08T15:02:39.538552",
     "status": "completed"
