   "cell_type": "code",
   "execution_count": 22,
   "id": "457535ba",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2024-07-05T14:59:24.876966Z",
     "iopub.status.busy": "2024-07-05T14:59:24.876536Z",
     "iopub.status.idle": "2024-07-05T14:59:24.887042Z",
