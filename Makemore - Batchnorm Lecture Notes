   "cell_type": "code",
   "execution_count": 22,
   "id": "50628ba7",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2024-07-08T14:57:53.740685Z",
     "iopub.status.busy": "2024-07-08T14:57:53.740297Z",
     "iopub.status.idle": "2024-07-08T14:57:53.749827Z",
